package days

import (
	"fmt"
	"myAwesomeModule/utils"
	"time"
)

func Day{DAYNUM}Part1() {
	utils.StartTimer()
	time.Sleep(1000)
	defer utils.WriteSolution("asd")
}

func Day{DAYNUM}Part2() {
	utils.StartTimer()
	time.Sleep(5000)
	defer utils.WriteSolution("fgh")
}

func Day{DAYNUM}() {
	Day{DAYNUM}Part1()
	Day{DAYNUM}Part2()

	fmt.Println(utils.GetDayID() + " done.")
}
